<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../layout/head')%>
</head>
<body>
    <header>
        <%- include('../layout/header')%>
    </header>
    <main>
        <h1>General Knowledge Quiz</h1>
        <% quest.forEach(question => { %>
            <h3 id="question"><%= question %></h3>
            <% data.forEach(choice => {%>
                <%- include('./partials/question', {choice} ) %>
            <% }) %>
        <% }) %>
    </main>
    <footer>
        <%- include('../layout/footer') %>
    </footer>
</body>

</html>